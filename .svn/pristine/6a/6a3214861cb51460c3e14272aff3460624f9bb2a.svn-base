package com.wb.model.pojo.computer;

public class CustomPayVo
{
    private String orgId;
    
    private String realAmount;
    
    private String payAmount;
    
    private String payment;
    
    private String discount;
    
    private String demo;
    
    private String payYear;
    
    private String payMonth;
    
    private String ifBookFee;
    
    private boolean auditFlag;
    
    private String accNO;
    
    private String conId;

    
    public String getConId() {
		return conId;
	}

	public void setConId(String conId) {
		this.conId = conId;
	}

	public String getAccNO() {
		return accNO;
	}

	public void setAccNO(String accNO) {
		this.accNO = accNO;
	}

	/**
     * 获取 ifBookFee
     * 
     * @return 返回 ifBookFee
     */
    public String getIfBookFee()
    {
        return ifBookFee;
    }
    
    /**
     * 设置 ifBookFee
     * 
     * @param 对ifBookFee进行赋值
     */
    public void setIfBookFee(String ifBookFee)
    {
        this.ifBookFee = ifBookFee;
    }

    public String getOrgId()
    {
        return orgId;
    }

    public void setOrgId(String orgId)
    {
        this.orgId = orgId;
    }

    public String getRealAmount()
    {
        return realAmount;
    }

    public void setRealAmount(String realAmount)
    {
        this.realAmount = realAmount;
    }

    public String getPayAmount()
    {
        return payAmount;
    }

    public void setPayAmount(String payAmount)
    {
        this.payAmount = payAmount;
    }

    public String getPayment()
    {
        return payment;
    }

    public void setPayment(String payment)
    {
        this.payment = payment;
    }

    public String getDiscount()
    {
        return discount;
    }

    public void setDiscount(String discount)
    {
        this.discount = discount;
    }

    public String getDemo()
    {
        return demo;
    }

    public void setDemo(String demo)
    {
        this.demo = demo;
    }

    public String getPayYear()
    {
        return payYear;
    }

    public void setPayYear(String payYear)
    {
        this.payYear = payYear;
    }

    public String getPayMonth()
    {
        return payMonth;
    }

    public void setPayMonth(String payMonth)
    {
        this.payMonth = payMonth;
    }

    public boolean isAuditFlag()
    {
        return auditFlag;
    }

    public void setAuditFlag(boolean auditFlag)
    {
        this.auditFlag = auditFlag;
    }
    
}

<script type="text/javascript">
function checkNull(){
	
	var userName=document.getElementById("name").value.replace(/(^\s*)|(\s*$)/g, "");
	if(!userName){
		$.showMsg("用户名不能为空,请填写！", 2);
		return false;
	}
	
	var position = document.getElementById("position").value.replace(/(^\s*)|(\s*$)/g, "");
	if(position==null||position==""){
		$.showMsg("职位不能为空！", 2);
		return false;
	}
	var telephone = document.getElementById("telephone").value.replace(/(^\s*)|(\s*$)/g, "");
	if(telephone==null||telephone==""){
		$.showMsg("电话不能为空！", 2);
		return false;
	}
	var email = document.getElementById("email").value.replace(/(^\s*)|(\s*$)/g, "");
	var myreg =/^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/;  
	if(email==null||email==""){
		$.showMsg("邮箱不能为空！", 2);
		return false;
	}
	if(!myreg.test(email)){
		$.showMsg("邮箱格式不正确！", 2);
		return false;
	  }
	
	var resume = document.getElementById("resume").value.replace(/(^\s*)|(\s*$)/g, "");
	if(resume==null||resume==""){
		$.showMsg("合作意向不能为空！", 2);
		return false;
	}
	
}
</script>




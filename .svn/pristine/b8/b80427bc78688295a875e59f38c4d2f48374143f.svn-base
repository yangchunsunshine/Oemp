package com.wb.component.computer.billManage.entity;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * 
 * 
 * @author ly
 * 
 */
public class BillMemberSettlement implements Serializable {

	private static final long serialVersionUID = 1L;

	private String id;

	private String memberId;

	private String memberName;

	private BigDecimal settlemented;

	private BigDecimal unsettlement;

	private BigDecimal totalAmount;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getMemberId() {
		return memberId;
	}

	public void setMemberId(String memberId) {
		this.memberId = memberId;
	}

	public String getMemberName() {
		return memberName;
	}

	public void setMemberName(String memberName) {
		this.memberName = memberName;
	}

	public BigDecimal getSettlemented() {
		return settlemented;
	}

	public void setSettlemented(BigDecimal settlemented) {
		this.settlemented = settlemented;
	}

	public BigDecimal getUnsettlement() {
		return unsettlement;
	}

	public void setUnsettlement(BigDecimal unsettlement) {
		this.unsettlement = unsettlement;
	}

	public BigDecimal getTotalAmount() {
		return totalAmount;
	}

	public void setTotalAmount(BigDecimal totalAmount) {
		this.totalAmount = totalAmount;
	}

	@Override
	public String toString() {
		return "BillMemberSettelment [id=" + id + ", memberId=" + memberId
				+ ", memberName=" + memberName + ", settlemented="
				+ settlemented + ", unsettlement=" + unsettlement
				+ ", totalAmount=" + totalAmount + "]";
	}

}

body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,form,input,table,tbody,p,th,td,em{-webkit-text-size-adjust:none;margin:0;padding:0;border:none;-webkit-tap-highlight-color:rgba(0,0,0,0);}
html,body{height:100%;}
ol,ul{list-style:none;} 
a:active, a:focus{outline:none;}
button::-moz-focus-inner,input::-moz-focus-inner{padding:0;border:0;}
*html{background-image:url(about:blank);background-attachment:fixed;} /* ÐÞÕýIE6Õñ¶¯bug */

/* slide */
.slide-main{height:630px;position:relative;}
.prev,.next{display:block;width:62px;height:80px;position:absolute;z-index:222;top:267px;overflow:hidden;cursor:pointer;opacity:0.6;-moz-opacity:0.6;filter:alpha(opacity=90);_border:1px solid none;}
.prev{left:75px;}
.next{right:75px;}
.prev:hover,.next:hover,.nav-main a:hover,.nav-main a.cur{opacity:1;-moz-opacity:1;filter:alpha(opacity=100);}
.item{display:block;width:100%;height:5px;position:absolute;z-index:124;top:595px;left:0;text-align:center;}
.item a{display:inline-block;width:10px;height:10px; border-radius:6px;margin-right:11px;background:#fff;overflow:hidden;border:1px solid #0278fc;}
.item a.cur{background:#0278fc;}
.slide-box,.slide{display:block;width:100%;height:630px;overflow:hidden;}
.slide-box{position:relative;}
.slide{display:none;height:630px;position:relative;position:absolute;z-index:8;}
